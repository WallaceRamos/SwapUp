{
  "expo": {
    "name": "SwapUp",
    "slug": "SwapUp",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "android": {
      "package": "com.wallaceramos.swapup",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptIcon.png" ,
        "backgroundColor": "#1193c8"

      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
